<% if (theme.aplayer.enable ){ %>
<!-- Aplayer 音乐播放器 -->
<link rel="stylesheet" href="//unpkg.com/aplayer@latest/dist/APlayer.min.css" />
<meting-js
  id="<%= theme.aplayer.id %>"
  server="<%= theme.aplayer.server %>"
  type="<%= theme.aplayer.type %>"
  fixed="<%= theme.aplayer.fixed %>"
  order="<%= theme.aplayer.order %>"
  preload="<%= theme.aplayer.preload %>"
>
</meting-js>
<script src="//unpkg.com/aplayer@latest/dist/APlayer.min.js"></script>
<!-- Meting 一个支持各种音乐平台(腾讯、网易...)的音乐 API 框架 -->
<script src="//unpkg.com/meting@latest/dist/Meting.min.js"></script>
<!-- Aplayer end -->
<% } %>
